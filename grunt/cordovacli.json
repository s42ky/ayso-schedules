{
  "options": {
    "path": ".",
    "cli": "cordova"
  },

  "prod": {
    "options": {
      "command": "build"
    }
  },

  "build_android": {
    "options": {
      "command": "build",
      "platforms": ["android"]
    }
  },
  "emulate_android": {
    "options": {
      "command": "emulate",
      "platforms": ["android"],
      "args": ["--target","Nexus5"]
    }
  }
}
