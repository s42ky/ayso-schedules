{
  "name": "AysoKS",
  "version": "2.0.0-alpha.4",
  "description": "Scheduling app for Wichita-area AYSO regions",
  "readme": "README.md",
  "license": "GPL-2.0",
  "repository": "https://github.com/s42ky/ayso-schedules.git",
  "scripts": {
    "clean": "rimraf build",
    "build": "webpack --progress --colors --display-error-details --display-cached",
    "build:profile": "webpack --progress --profile --colors --display-error-details --display-cached --devo",
    "build:android": "webpack --progress --profile --colors --display-error-details --display-cached --devo --android",
    "watch": "webpack --progress --profile --colors --display-error-details --display-cached --devo --watch",
    "img": "cp res/img/* build/img & cp src/img/* build/img",
    "test": "karma start karma.conf.js",
    "release": "npm run test && npm run release:web",
    "release:web": "webpack --progress --profile --colors --display-error-details --display-cached --prod",
    "release:android": "webpack --progress --profile --colors --display-error-details --display-cached --prod --android",
    "server": "webpack-dev-server --progress --profile --colors --display-error-details --no-info --content-base build/",
    "start": "npm run",
    "android": "npm run build:android && npm run cordova",
    "cordova": "npm run cordova:init && npm run cordova:android",
    "cordova:init": "(mkdir hooks platforms plugins www || pwd) && cordova prepare",
    "cordova:make": "cordova compile",
    "cordova:android": "cordova run android"
  },
  "dependencies": {
    "angular2": "^2.0.0-beta.2",
    "angular2-google-maps": "^0.6.1",
    "bootstrap": "^4.0.0-alpha.2",
    "cordova": "^6.0.0",
    "cordova-plugin-network-information": "^1.0.1",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.13",
    "es7-reflect-metadata": "^1.4.0",
    "ionicons": "^2.0.1",
    "jquery": "^2.1.4",
    "jquery-mobile": "^1.4.1",
    "ng2-bootstrap": "^0.53.0",
    "rxjs": "5.0.0-beta.0",
    "systemjs": "^0.19.9",
    "tether": "^1.1.1",
    "zone.js": "0.5.10"
  },
  "devDependencies": {
    "angular-jsdoc": "^1.2.1",
    "bootstrap-loader": "^1.0.5",
    "cca": "^0.8.1",
    "concurrently": "^1.0.0",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.7.1",
    "imports-loader": "^0.6.5",
    "istanbul-instrumenter-loader": "^0.1.3",
    "jasmine": "^2.3.2",
    "jasmine-core": "2.4.1",
    "jasmine-expect": "^2.0.2",
    "karma": "^0.13.14",
    "karma-chrome-launcher": "^0.2.1",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.6",
    "karma-jasmine-html-reporter": "^0.2.0",
    "karma-jasmine-matchers": "^2.0.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "lite-server": "^2.0.1",
    "node-sass": "^3.4.2",
    "phantomjs": "^2.1.3",
    "phantomjs-polyfill": "0.0.1",
    "plugman": "^1.0.4",
    "postcss-loader": "^0.8.0",
    "protractor": "^3.0.0",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "0.1.2",
    "remap-istanbul": "^0.5.1",
    "resolve-url-loader": "^1.4.3",
    "rimraf": "^2.5.0",
    "sass-loader": "^3.1.2",
    "script-loader": "^0.6.1",
    "style-loader": "^0.13.0",
    "svgo": "^0.6.2",
    "svgo-loader": "^1.1.0",
    "ts-helper": "0.0.1",
    "ts-loader": "^0.8.0",
    "tsconfig-lint": "^0.5.0",
    "tsd": "^0.6.5",
    "tslint": "^3.2.1",
    "tslint-loader": "^2.1.0",
    "typescript": "^1.7.5",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.11",
    "webpack-cordova-plugin": "^0.1.6",
    "webpack-dev-server": "^1.14.1"
  }
}
